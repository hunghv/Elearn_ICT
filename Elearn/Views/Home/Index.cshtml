@{  
    ViewBag.Title = "Welcome";  
}  
  
<style>  
    table, tr, td, th {  
        border: 1px solid #ccc;  
        padding: 10px;  
        margin: 10px;  
    }  
</style>  
<h2>Welcome to Sibeesh Passion's Email List</h2>  
<body data-ng-app="APIModule">  
<div id="tblSubs" ng-controller="APIController">  
    <table>  
        <tr>  
            <th>ID</th>  
            <th>Email ID</th>  
            <th>Subscribed Date</th>  
        </tr>  
        <tbody data-ng-repeat="sub in subscriber">  
        <tr>  
            <td>{{sub.Id}}</td>  
            <td>{{sub.Name}}</td>  
            <td>{{sub.Description}}</td>  
        </tr>  
        </tbody>  
    </table>  
</div>  
</body>  
<script src="~/Assets/Scripts/angular.js"></script>  
<script src="~/Assets/Scripts/angular-route.js"></script>  
<script src="~/Assets/App/APIScipts/Module.js"></script>
<script src="~/Assets/App/APIScipts/Services.js"></script>
<script src="~/Assets/App/APIScipts/Controller.js"></script>
